[global_config]
  always_split_with_profile = True
  dbus = False
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler
  focus = system
  handle_size = 0
  homogeneous_tabbar = False
  inactive_color_offset = 0.79
  scroll_tabbar = True
  suppress_multiple_term_dialog = True
  title_font = Source Code Pro Bold 10
  title_transmit_bg_color = "#90b132"
[keybindings]
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  go_up = <Alt>k
  group_all_toggle = <Super>g
  layout_launcher = <Alt>semicolon


#########################
######## LAYOUTS ########
#########################

[layouts]

  [[default]]
    [[[child1]]]
